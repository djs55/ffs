#!/usr/bin/env python

import sys, urlparse
import xapi, d

class Implementation(d.Datapath_skeleton):
    def attach(self, dbg, uri, domain):
        u = urlparse.urlparse(uri)
        return {
            'domain_uuid': '0',
            'implementation': [ 'Qdisk', u.path ],
        }

if __name__ == "__main__":
    cmd = d.Datapath_commandline(Implementation())
    cmd.attach()
